generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String    @id @default(uuid())
  email       String    @unique
  firstName   String?
  lastName    String?
  phoneNumber String?
  dateOfBirth DateTime?
  mobile      String
  password    String
  createdAt   DateTime  @default(now())
}
